                android:value=".service.activity.MainActivity" />
        </activity>
        <activity
            android:name=".service.activity.SignUpActivity"
            android:label="@string/title_activity_sign_up"
            android:parentActivityName=".service.activity.SignUpPreviousActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
